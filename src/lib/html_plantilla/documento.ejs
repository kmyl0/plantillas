  <body class="<%= !html? 'globalPdf': 'globalHtml' %>">


    <%

    var marcaAgua = true;
      var roles = audit_usuario.roles;
      for(var a = 0; a < roles.length; a++){
        if(roles[a].fid_rol == 4){
          marcaAgua = false;
          break;
        }
      }
      if(html){
        marcaAgua = false;
      }
    %>
